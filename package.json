{
  "name": "game-note",
  "version": "1.0.0",
  "description": "Game Note web APP",
  "main": "src/index.js",
  "scripts": {
    "develop": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "prettier": "prettier --check ./src/",
    "eslint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "format": "prettier --write ./src/",
    "test": "jest",
    "pre-commit": "npm run prettier && npm run eslint && npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "\\.js$": [
        "babel-jest"
      ]
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.css$": "identity-obj-proxy",
      "^game-note(.*)$": "<rootDir>/src/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gaitz/GameNote.git"
  },
  "author": "PHC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gaitz/GameNote/issues"
  },
  "homepage": "https://github.com/Gaitz/GameNote#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.1",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@reduxjs/toolkit": "^1.4.0",
    "firebase": "^7.21.0",
    "normalize.css": "^8.0.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0"
  }
}
